<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns= "http://www.springframework.org/schema/beans"
    xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans      
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
    
    <bean id = "adr" class = "com.test.Address">
    <property name = "street" value = "HauzKhas"></property>
    <property name ="city" value ="Patna"></property>
    <property name ="state" value ="Bihar"></property>
    </bean>
    
    <bean id = "emp" class = "com.test.Employee">
    <property name = "id" value = "1"></property>
    <property name = "name" value = "Samrat"></property>
    <property name = "cmp" value = "CGI"></property>
    <property name = "adr" ref = "adr"></property>
    </bean>

</beans>